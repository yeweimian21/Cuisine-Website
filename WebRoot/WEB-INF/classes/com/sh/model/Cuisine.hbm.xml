<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.sh.model">
	<class name="Cuisine" table="Cuisine">
		<id name="cuisineId" column="cuisineId" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">cuisineIdSequence</param>
			</generator>
		</id>
		
		<property name="cuisineName" column="cuisineName"
			type="java.lang.String" length="20" not-null="true"/>
	</class>
</hibernate-mapping>
