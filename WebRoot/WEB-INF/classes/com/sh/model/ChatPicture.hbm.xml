<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.sh.model">
	<class name="ChatPicture" table="ChatPicture">
		<composite-id>
			<key-property name="chatId" column="chatId" type="java.lang.Integer"></key-property>
			<key-property name="chatPicture" column="chatPicture" type="java.lang.String" length="100"></key-property>
		</composite-id>
	</class>
</hibernate-mapping>
