<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.sh.model">
	<class name="MaterialCategory" table="MaterialCategory">
		<id name="materialCategoryId" column="materialCategoryId" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">materialCategoryIdSequence</param>
			</generator>
		</id>
		
		<property name="materialCategoryName" column="materialCategoryName"
			 type="java.lang.String" length="20" not-null="true"/>
			 
	</class>
</hibernate-mapping>
