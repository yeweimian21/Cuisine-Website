<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.sh.model">
	<class name="CostTime" table="CostTime">
		<id name="costTimeId" column="costTimeId" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">costTimeIdSequence</param>
			</generator>
		</id>
		
		<property name="costTimeName" column="costTimeName"
			type="java.lang.String" length="20" not-null="true"/>
	</class>
</hibernate-mapping>
