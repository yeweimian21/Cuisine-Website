<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.sh.model">
	<class name="Hottest" table="Hottest">
		<id name="hottestId" column="hottestId" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">hottestIdSequence</param>
			</generator>
		</id>
		
		<property name="recipeId" column="recipeId"
			type="java.lang.Integer" not-null="true"/>
	</class>
</hibernate-mapping>
